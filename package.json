    "react-instantsearch-dom": "^6.6.0",
    "react-tooltip": "^4.2.17",
    "styled-components": "^5.1.1",
    "styled-icons": "^10.33.0",
    "styled-media-query": "^2.1.2",
    "typescript": "^4.2.4"
